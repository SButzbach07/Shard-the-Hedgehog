// Switch
sprite SD_SWITCH
{
    source_file     images/switches.png
    source_rect     288 64 64 8
    frame_size      32 8
    hot_spot        16 8

    // default
    animation 0
    {
        repeat      TRUE
        fps         8
        data        0
    }

    // pressed
    animation 1
    {
        repeat      TRUE
        fps         8
        data        1
    }
}
